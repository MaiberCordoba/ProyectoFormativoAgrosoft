import { useQuery } from "@tanstack/react-query";
import { getSemilleros } from "../../api/SemillerosApi";
import { Semilleros } from "../../types";

export const useGetSemilleros = () => {
  return useQuery<Semilleros[], Error>({
    queryKey: ["Semilleros"], 
    queryFn: getSemilleros, 
  });
};

